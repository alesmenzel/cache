{
  "name": "@alesmenzel/cache",
  "version": "1.0.0-rc.3",
  "description": "Async function cache",
  "license": "MIT",
  "keywords": [
    "cache",
    "memoize",
    "redis",
    "in-memory"
  ],
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write \"./**/*.{js,json,css,md}\"",
    "test": "cross-env DEBUG= jest",
    "prepublishOnly": "run-s lint test",
    "publish:latest": "npm publish --access public --tag latest",
    "publish:next": "npm publish --access public --tag next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alesmenzel/cache"
  },
  "bugs": {
    "url": "https://github.com/alesmenzel/cache/issues"
  },
  "homepage": "https://github.com/alesmenzel/cache",
  "contributors": [
    "Ale≈° Menzel <alesmenzel@gmail.com>"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "async": "^2.6.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "redis": "^2.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ]
